import com.example.fw.ApplicationManager;

public class TestBase {
	protected ApplicationManager app;
  

	@BeforeGroups
	public void setUp() throws Exception {
		app = new ApplicationManager();
	   	  }
	@AfterGroups
	public void tearDown() throws Exception {
		app.stop();
	   
	  }

}
